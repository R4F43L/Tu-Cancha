<%= link_to 'Registrar nuevo local', new_place_path %></br></br></br>

<h1>Mis locales</h1>


<table>
  <tr>
    <th>Usuario</th>
	<th>Nombre Descriptivo</th>
    <th>Dirección</th>
    <th>Distrito</th>
	<th>Dirección en Google Maps</th>
    <th>Teléfono</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each  do |place| %>
  <tr>
    <td><%= place.person_id %></td>
	<td><%= place.nameplace %></td>
    <td><%= place.address %></td>
    <td><%= place.district %></td>
	<td><INPUT type="button" value="Ver en Google Maps!" onClick="window.open('<%= place.gmaddress %>','mywindow','width=1024,height=768')"></td>
    <td><%= place.telephone %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

